%h1= "n00bz"
- if @players
  - @players.each do |player|
    %img{:src => player.avatar}
    %a(href="http://www.tf2items.com/profiles/#{player.steam_id}") #{player.name}
    %br
    - player.inventories.each do |inv|
      = "#{inv.item.name} : #{inv.item_count}"
    %br
  %br
= button_to "find some n00bz", :action => "create"
